	<style name="skin_hotspotstyle1"
        scale="0.5"
        zorder="50"
        handcursor="true"
        onhover="showtext(Disponible · Ver ficha);"
        alpha="1"
        type="text"
        edge="center"
        css="font-family:Din Alternate; font-size:21px; color:#000000;font-weight: bold; text-align:center;"
        vcenter="true"
        padding="5"
        bg="true"
        bgcolor="0xffffff"
        bgalpha="1.0"
        bgborder="0"
        bgroundedge="22"
        bgshadow=""
        txtshadow=""
        width="44" height="44"
